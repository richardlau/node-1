{
    "problemMatcher": [
        {
            "owner": "lint-md",
            "pattern": [
                {
                    "regexp": "^([^\\s].*)$",
                    "file": 1
                },
                {
                    "regexp": "^\\s+(?:\\d+:\\d+-)?(\\d+):(\\d+)\\s+(\\S+)\\s+(.+)\\s+(\\S+)\\s+(?:\\S+)$",
                    "line": 1,
                    "column": 2,
                    "severity": 3,
                    "message": 4,
                    "code": 5,
                    "loop": true
                }
            ]
        }
    ]
}
