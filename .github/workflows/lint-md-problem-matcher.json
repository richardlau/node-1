{
    "problemMatcher": [
        {
            "owner": "lint-md",
            "pattern": [
                {
                    "regexp": "^(.*)$",
                    "file": 1
                },
                {
                    "regexp": "^\\s+(?:\\d+:\\d+-)?(\\d+):(\\d+)\\s+(error|warning|info)\\s+(.+)\\s+([^\\s]+)\\s+(?:[^\\s]+)$",
                    "line": 1,
                    "column": 2,
                    "severity": 3,
                    "message": 4,
                    "code": 5,
                    "loop": true
                }
            ]
        }
    ]
}
